  "name": "codacy-eslint",
  "description": "Eslint Tool for Codacy",
  "keywords": [
    "codacy",
    "eslint",
    "javascript",
    "linter",
    "typescript"
  ],
  "homepage": "https://github.com/codacy/codacy-eslint#readme",
  "bugs": {
    "url": "https://github.com/codacy/codacy-eslint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codacy/codacy-eslint.git"
  },
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "directories": {
    "test": "./src/test",
    "src": "./src"
  },
  "scripts": {
    "build": "tsc --showConfig && tsc",
    "build:docker": "docker build -t codacy-eslint:dev .",
    "build:docker:m1": "npm run build:docker -- --platform linux/amd64",
    "build:docs": "npm run build && node dist/src/docGeneratorMain.js",
    "lint": "eslint --fix --ext .ts ./src",
    "test": "mocha",
    "preupgrade": "npx npm-check-updates -u -x @teambit/react.eslint-config-bit-react,eslint && npx npm-check-updates -u -t minor eslint,eslint-plugin-lodash",
    "upgrade": "npm install --legacy-peer-deps --omit=dev",
    "postupgrade": "npm run build:docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "npm run build:docs",
      "sort-package-json",
      "git add"
    ],
    "src/*.{ts,js,css,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "require": "ts-node/register",
    "spec": "src/test/**/*.spec.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
