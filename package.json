  },
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "directories": {
    "test": "./src/test",
    "src": "./src"
  },
  "scripts": {
    "compile": "tsc --showConfig && tsc",
    "dockerBuild": "docker build -t codacy-eslint .",
    "generateDocs": "npm run compile && node dist/src/docGeneratorMain.js",
    "build": "tsc --showConfig && tsc",
    "build:docker": "npm run build && docker build -t codacy-eslint .",
    "build:docker:m1": "npm run build && docker build --platform linux/amd64 -t codacy-eslint .",
    "build:docs": "npm run build && node dist/src/docGeneratorMain.js",
    "test": "mocha",
    "update": "ncu -u -x @teambit/react.eslint-config-bit-react,eslint-plugin-playwright,node-fetch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "npm run build:docs",
      "sort-package-json",
      "git add"
    ],
    "src/*.{ts,js,css,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "require": "ts-node/register",
    "spec": "src/test/**/*.spec.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
